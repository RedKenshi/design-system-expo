{
  "expo": {
    "name": "design-system",
    "slug": "design-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "designsystemexpo",
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Axiforma Thin.otf",
            "./assets/fonts/Axiforma Thin Italic.otf",
            "./assets/fonts/Axiforma Light.otf",
            "./assets/fonts/Axiforma Light Italic.otf",
            "./assets/fonts/Axiforma Book.otf",
            "./assets/fonts/Axiforma Book Italic.otf",
            "./assets/fonts/Axiforma Regular.otf",
            "./assets/fonts/Axiforma Italic.otf",
            "./assets/fonts/Axiforma Medium.otf",
            "./assets/fonts/Axiforma Medium Italic.otf",
            "./assets/fonts/Axiforma SemiBold.otf",
            "./assets/fonts/Axiforma SemiBold Italic.otf",
            "./assets/fonts/Axiforma Bold.otf",
            "./assets/fonts/Axiforma Bold Italic.otf",
            "./assets/fonts/Axiforma ExtraBold.otf",
            "./assets/fonts/Axiforma ExtraBold Italic.otf",
            "./assets/fonts/Axiforma Black.otf",
            "./assets/fonts/Axiforma Black Italic.otf",
            "./assets/fonts/Axiforma Heavy.otf",
            "./assets/fonts/Axiforma Heavy Italic.otf"
          ]
        }
      ],
      "expo-router"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redkenshi.designsystem",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "Use this to communicate with the printer.",
        "NSLocalNetworkUsageDescription": "Use this to says Bonjour to the printer.",
        "UISupportedExternalAccessoryProtocols": [
          "com.epson.escpos"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.redkenshi.designsystem"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7a541383-8043-44f2-9e65-dccd8f0cc093"
      }
    }
  }
}